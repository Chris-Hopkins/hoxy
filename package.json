    "https-proxy-agent": "^2.2.2",
    "lodash": "^4.17.0",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "node-static": "^0.7.6",
    "nodemon": "^2.0.2",
    "pem": "^1.7.2",
    "route-pattern": "^0.0.6",
    "stream-throttle": "^0.1.3",
    "ugly-adapter": "^1.1.0"
